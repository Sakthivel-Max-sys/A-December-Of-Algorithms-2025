def next_greater_to_right(heights):
    n = len(heights)
    result = [-1] * n
    stack = []  # will store indices

    for i in range(n):
        while stack and heights[stack[-1]] < heights[i]:
            result[stack.pop()] = heights[i]
        stack.append(i)

    return result


# Input Handling
N = int(input())
heights = list(map(int, input().split()))

answer = next_greater_to_right(heights)
print(*answer)

